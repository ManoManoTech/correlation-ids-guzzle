{
    "name": "manomano-tech/correlation-ids-guzzle",
    "description": "Guzzle plugin for manomano-tech/correlation-ids package",
    "keywords": [
        "correlation",
        "correlation-id",
        "tracing",
        "guzzle",
        "guzzle6",
        "plugin",
        "middleware"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.0",
        "manomano-tech/correlation-ids": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symfony/debug": "^4.1",
        "symfony/phpunit-bridge": "^4.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ManoManoTech\\CorrelationIdGuzzle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ManoManoTech\\CorrelationIdGuzzle\\Tests\\": "tests/"
        }
    }
}

